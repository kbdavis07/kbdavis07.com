<header itemscope="itemscope" itemtype="http://schema.org/Person" class="navbar-fixed-top">

    <div class="row">

        <div id="Pic" class="col-md-1">
            <img data-src="holder.js/100x100" alt="Brian Keith Davis"
                 src="https://secure.gravatar.com/avatar/fb1016f89aaf56e0137d00ccc040c75e?s=64"
                 itemprop="image" class="media-object img-circle center-block profile-pic" />
        </div>



        <div class="col-md-4">
            <h5 itemprop="name"><b>Brian Keith Davis</b></h5>
            <h4 itemprop="jobTitle">Asp.Net C# MVC Web Application Developer</h4>

            <a href="https://www.linkedin.com/in/kbdavis07" target="_blank" rel="tooltip" title="LinkedIn"
               class="fs-2x social-link link-linkedin icon-linkedin"></a>

            <a href="https://github.com/kbdavis07" target="_blank" rel="tooltip" title="GitHub"
               class="fs-2x social-link link-github icon-github"></a>

            <a href="https://bitbucket.org/kbdavis07/" target="_blank" rel="tooltip" title="BitBucket"
               class="fs-2x social-link link-bitbucket icon-bitbucket"></a>

            <a href="http://stackoverflow.com/users/783230/kbdavis07" target="_blank" rel="tooltip" title="StackOverFlow"
               class="fs-2x social-link link-stackoverflow icon-stackoverflow"></a>

            <a href="http://twitter.com/kbdavis07" target="_blank" rel="tooltip" title="Twitter"
               class="fs-2x social-link link-twitter icon-twitter"></a>

            <a href="skype:kbdavis07?call" target="_blank" rel="tooltip" title="Skype"
               class="fs-2x social-link link-skype icon-skype"></a>

        </div>

        <div id="header-image" class="col-md-6"></div>


    </div>

    <nav class="navbar navbar-default" role="navigation">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
